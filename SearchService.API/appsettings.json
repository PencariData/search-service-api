{
  "ElasticConfiguration": {
    "ElasticUrl": "http://localhost:9200",
    "AccommodationIndex": "accommodations",
    "DestinationIndex": "destinations"
  },
  "Caching": {
    "SuggestionCacheDurationMinutes": 1440,
    "ResultCacheDurationMinutes": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
